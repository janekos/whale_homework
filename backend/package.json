    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.10.2",
    "@types/supertest": "^6.0.3",
    "jest": "^30.1.3",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
