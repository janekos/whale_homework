  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d data-source.ts",
